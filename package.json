{
  "name": "@baleada/vue-prose",
  "version": "0.13.1",
  "description": "Vue components that enhance prose for the Baleada toolkit",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    },
    "./plugin": {
      "import": "./plugin/index.js"
    },
    "./propsInterfaces": {
      "import": "./propsInterfaces/index.js",
      "require": "./propsInterfaces/index.cjs"
    },
    "./loopedIdPrefix": {
      "import": "./loopedIdPrefix/index.js",
      "require": "./loopedIdPrefix/index.cjs"
    }
  },
  "scripts": {
    "prepare": "rollup --config rollup.config.js",
    "test:only": "run() { npx uvu tests/$2 $1.test.js$; }; run",
    "test": "npm run test:only .",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baleada/vue-prose.git"
  },
  "keywords": [
    "baleada",
    "prose",
    "markdown",
    "components",
    "accessible",
    "vue"
  ],
  "author": "Alex Vipond",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baleada/vue-prose/issues"
  },
  "homepage": "https://baleada.netlify.com",
  "sideEffects": false,
  "devDependencies": {
    "@baleada/prepare": "^0.4.15",
    "@vue/compiler-sfc": "^3.0.4",
    "rollup": "^2.33.1",
    "uvu": "^0.5.1",
    "vite": "^1.0.0-rc.13",
    "vue-router": "^4.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@baleada/vue-composition": "^0.8.4",
    "@baleada/vue-features": "^0.5.1",
    "@baleada/vue-heroicons": "^0.0.2",
    "@baleada/vue-interface": "^0.7.2",
    "@baleada/vue-simple-icons": "^0.0.0",
    "vue": "^3.0.2"
  }
}
